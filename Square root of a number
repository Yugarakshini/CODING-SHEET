

/*You are required to complete
this function*/

// Function to find square root
// x: element to find square root
class Solution {
    long floorSqrt(long n) {
        // Your code here
        long i;
        for(i=1;i*i<=n;i++){
            if(i*i>=n){
                break;
            }
        }
        if(i*i==n){
            return i;
        }else{
            return i-1;
        }
    }
}
