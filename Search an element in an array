class Solution {

    static int search(int arr[], int x) {
        int n = arr.length;
        // iterate over the array elements
        for (int i = 0; i < n; i++) {
            // check if array element is equal to x
            if (arr[i] == x) {
                return i;
            }
        }

        // return -1 if x is not found in the array
        return -1;
    }
}
